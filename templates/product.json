{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "product_meta": {
          "type": "product_meta",
          "settings": {
            "show_vendor": true,
            "show_sku": true,
            "show_reviews_badge": false,
            "show_share_buttons": true
          }
        },
        "text_FYDwhT": {
          "type": "text",
          "settings": {
            "content": "<p>Add your text here<\/p>"
          }
        },
        "variant_selector": {
          "type": "variant_selector",
          "settings": {
            "selector_mode": "block",
            "color_mode": "color",
            "show_quantity_selector": true,
            "show_taxes_included": false,
            "show_inventory_quantity": false,
            "low_inventory_threshold": 0
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_payment_button": false
          }
        },
        "description": {
          "type": "description",
          "disabled": true,
          "settings": {
            "display_mode": "show_all"
          }
        },
        "liquid_Tbd46i": {
          "type": "liquid",
          "settings": {
            "title": "Tracklist",
            "sku": "",
            "liquid": "{{product.metafields.meta.tracklist}}",
            "display_mode": "collapse"
          }
        }
      },
      "block_order": [
        "product_meta",
        "text_FYDwhT",
        "variant_selector",
        "buy_buttons",
        "description",
        "liquid_Tbd46i"
      ],
      "settings": {
        "list_collections": true,
        "image_size": "small",
        "enable_video_looping": false,
        "enable_image_zoom": true,
        "zoom_effect": "outside",
        "carousel_effect": "fade"
      }
    },
    "custom_liquid_RgYmJH": {
      "type": "custom-liquid",
      "settings": {
        "title": "",
        "include_margins": true,
        "liquid": "<h3 class=\"h2\">Descrizione<\/h3>\n<p>{{product.content}}<\/p>"
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
        "show_quick_buy": false
      }
    },
    "recently-viewed-products": {
      "type": "recently-viewed-products",
      "settings": {
        "title": "Recently viewed",
        "show_quick_buy": false
      }
    }
  },
  "order": [
    "main",
    "custom_liquid_RgYmJH",
    "product-recommendations",
    "recently-viewed-products"
  ]
}
